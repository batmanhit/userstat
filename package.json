{
  "name": "userstats",
  "version": "0.0.1",
  "description": "API that persists daily stats of compare users usage",
  "main": "index.js",
  "scripts": {
    "test": "dotenv -e .env.default tap",
    "offline": "dotenv -e .env.default serverless offline -- --httpsProtocol certs",
